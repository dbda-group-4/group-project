{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "Creating Lambda",
    "Resources": {
        "MyLambdaFunction": {
            "Type": "AWS::Lambda::Function",
            "Properties": {
                "Handler": "lambda.lambda_handler",
                "Role": "arn:aws:iam::550069569828:role/s3-rds-role",
                "FunctionName": {
                    "Ref": "functionname"
                },
                "Code": {
                    "S3Bucket": "",
                    "S3Key": "lambda.zip"
                },
                "Runtime": "python3.6",
                "Timeout": "25"
            }
        }
    },
    "Parameters": {
        "functionname": {
            "Type": "String"
        }
    }
}